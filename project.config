[access]
	inheritFrom = All-Projects
[project]
	description = Fabric CA
[access "refs/*"]
	read = group GitHub Replication
	owner = group ldap/hyperledger-gerrit-fabric-committers
[access "refs/tags/*"]
	push = group ldap/hyperledger-gerrit-fabric-committers
	pushTag = group ldap/hyperledger-gerrit-fabric-committers
	pushSignedTag = group ldap/hyperledger-gerrit-fabric-committers
