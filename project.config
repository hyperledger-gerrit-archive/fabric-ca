[access]
	inheritFrom = All-Projects
[project]
	description = Fabric CA
	state = read only
[access "refs/*"]
	read = group GitHub Replication
	owner = group ldap/hyperledger-gerrit-fabric-ca-committers
	owner = group ldap/hyperledger-gerrit-fabric-release-maintainers-committers
[access "refs/heads/*"]
	create = group ldap/hyperledger-gerrit-fabric-ca-committers
	create = group ldap/hyperledger-gerrit-fabric-release-maintainers-committers
[access "refs/tags/*"]
	push = group ldap/hyperledger-gerrit-fabric-ca-committers
	push = group ldap/hyperledger-gerrit-fabric-release-maintainers-committers
	createTag = group ldap/hyperledger-gerrit-fabric-ca-committers
	createTag = group ldap/hyperledger-gerrit-fabric-release-maintainers-committers
	createSignedTag = group ldap/hyperledger-gerrit-fabric-ca-committers
	createSignedTag = group ldap/hyperledger-gerrit-fabric-release-maintainers-committers
